CREATE DATABASE `BRAINPOT`;
USE `BRAINPOT`;


CREATE TABLE `USER` (
	  `ID` INT NOT NULL UNIQUE AUTO_INCREMENT,
    `TEAM` INT NOT NULL,
    `ADMIN` BOOLEAN NOT NULL,
    `NICKNMAE` CHAR(10) NOT NULL,
    FOREIGN KEY(`TEAM`) REFERENCES `TEAM`(`ID`) ON UPDATE CASCADE ON DELETE CASCADE,

    KEY `IDX_ID`(`ID`)
);

#검색 쿼리
SELECT * FROM `USER`;


#### DANGER!! ####

DROP TABLE `USER`;